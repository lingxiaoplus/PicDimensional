<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="WaveLoading">
        <attr name="mainColor" format="color"></attr>
        <attr name="text" format="string"></attr>
    </declare-styleable>

    <!--设置界面的组合控件
        title 大标题
        message内容
        switch 开关
        visable 开关是否可见
    -->
    <declare-styleable name="SettingCardView">
        <attr name="title" format="string"/>
        <attr name="message" format="string"/>
        <attr name="switchcompat" format="boolean"/>
        <attr name="visable" format="boolean"/>
    </declare-styleable>
</resources>